{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "plugin: react/recommended",
    "airbnb"
  ],
  "globals": {
    "Atomics": "readonly",
    "ShareArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "semi": ["error", "never"],
    "react/jsx-filename-extension": ["off"]
  }
}